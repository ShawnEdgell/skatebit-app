{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "XLFM",
  "version": "0.9.0",
  "identifier": "com.tauri-app.app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../build"
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "icon": ["icons/icon.icns", "icons/icon.ico"],
    "targets": ["nsis"],
    "windows": {
      "certificateThumbprint": null
    }
  },
  "plugins": {
    "updater": {
      "installMode": "passive",
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJBMjRCQTZGODU2NTE5REEKUldUYUdXV0ZiN29rS29xaDc1R2lxT3AyNXZGdkZFakJjMHFzSE1Pd0MzZDl2T25KeWMranJBSjYK",
      "endpoints": [
        "https://github.com/ShawnEdgell/skatebit-app/releases/latest/download/latest.json"
      ]
    },
    "os": {},
    "shell": {},
    "fs": {}
  },
  "app": {
    "security": {
      "capabilities": ["main-capability"],
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "XL File Manager",
        "width": 900,
        "height": 640,
        "label": "main",
        "transparent": false,
        "decorations": false
      }
    ]
  }
}
